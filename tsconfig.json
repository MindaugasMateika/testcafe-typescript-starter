{
  "compilerOptions": {
    "allowUnusedLabels": false,
    "baseUrl": "./",
    "paths": {
      "testcafe/*": ["./testcafe/*"],
      "core": ["./core/*"],
      "config/*": ["./config/*"]
    },
    "lib": ["dom", "es6", "es7"],
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "outDir": "./dist",
    "removeComments": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "mapRoot": "./dist",
    "esModuleInterop": true,
    "target": "es5",
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "testcafe/ts-defs", "jquery"]
  },
  "exclude": ["node_modules"]
}
